@model IEnumerable<SolveOrDrinkIt.Models.Score>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.userId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.score1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Game.id)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.userId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.score1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Game.id)
        </td>
    </tr>
}

</table>
